{
    "extends": "standard",
    "plugins": ["react", "react-hooks"],
    "rules": {
        "no-use-before-define": "off",
        "array-callback-return": "off",
        "no-unused-vars": "off",
        "no-return-assign": "off"
    },
    "env": {
        "browser": true,
        "node": true
    },
    "parser": "@typescript-eslint/parser",
    "settings": {
        "import/resolver": {
            "node": {
                "paths": ["src"]
            }
        }
    }
}